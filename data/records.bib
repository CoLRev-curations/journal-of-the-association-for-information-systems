   colrev_data_provenance        = {url:DBLP.bib/000784;;
                                    language:http://dx.doi.org/10.17705/1JAIS.00789;;
                                    doi:https://aisel.aisnet.org/jais/vol24/iss2/7;;
                                    abstract:http://dx.doi.org/10.17705/1JAIS.00789;;
