   colrev_pdf_id                 = {cpid2:fffffffffdfffffff87ffc0ff8400007fdc0000ffffffffffffffffff800001ff000000fffc183ffffc003ffff8001fffffffffffc00003ffc00003ffc00003ffc00003ffc00003ffc01203ffcfffffff8e0003ff005ff6ff0ff908ff8fb800ff803800ff003800ff003800ff003800ff003800ffbffa98fffffffffffffffff},
   file                          = {data/pdfs/22_2/StrichMayerFiedler2021a.pdf},
