   colrev_pdf_id                 = {cpid2:fffffffffdfffffff87ffc0ff8400007fdc0000ffffffffffffffffff800001ffe00007fff8805ffff8001ffff8001ffff8001ffff8001fffffe7ffff800003ffc00003ffc00003ffc00003ffc00003ffc00003ffc00003ffc001ffffc0ffffff000000ff1fffffff87f800ff001800ff001800fffff810fffffffffffffffff},
