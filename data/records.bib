                                    doi:https://aisel.aisnet.org/jais/vol24/iss3/6;;
                                    abstract:http://dx.doi.org/10.17705/1JAIS.00807;;
                                    colrev.dblp.dblp_key:DBLP.bib/000793|rename-from:dblp_key;;
