   colrev_data_provenance        = {doi:https://api.crossref.org/works/10.17705/1JAIS.00696;;
                                    abstract:https://api.crossref.org/works/10.17705/1JAIS.00696;;
                                    language:https://api.crossref.org/works/10.17705/1JAIS.00696;;
                                    cited_by:https://api.crossref.org/works/10.17705/1JAIS.00696;;
