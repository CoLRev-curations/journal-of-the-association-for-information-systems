   colrev_pdf_id                 = {cpid2:fffffffffdfffffff87ffc0ff8400007fdc0000ffffffffffffffffff000001ffe00007fffc003ffffe00bfffe2000ffff8001ffff0000ffff0530fffffe7ffffc00003ffc00003ffc00003ff800003ffc00003ffc00003ffc00003ffc0000fffc00037ff800003ff0007b4ff07ffffff87f800ff381ffffffffffffffffffff},
