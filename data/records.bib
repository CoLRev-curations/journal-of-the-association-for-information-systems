@article{ErikssonOhlund2024,
   colrev_origin                 = {CROSSREF.bib/000841;
                                    DBLP.bib/000841;
                                    pdfs.bib/000818;},
   colrev_status                 = {md_processed},
   colrev_masterdata_provenance  = {author:CROSSREF.bib/000841;;
                                    journal:CROSSREF.bib/000841;;
