   colrev_data_provenance        = {url:DBLP.bib/000750;;
                                    language:http://dx.doi.org/10.17705/1JAIS.00747;;
                                    doi:https://aisel.aisnet.org/jais/vol23/iss4/6;;
                                    abstract:http://dx.doi.org/10.17705/1JAIS.00747;;
