                                    doi:https://aisel.aisnet.org/jais/vol24/iss4/6;;
                                    abstract:http://dx.doi.org/10.17705/1JAIS.00819;;
                                    colrev.dblp.dblp_key:DBLP.bib/000801|rename-from:dblp_key;;
