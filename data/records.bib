   colrev_pdf_id                 = {cpid2:fffffffffdfffffff87ffc0ff8400007fdc0000fffffffffffffffffff8001fffc00003fff3190fffc00003ffc00003ffffefffffffe7ffffc00003ffc00003ffc00003ffc00003ffc00003ffc00003ffc000ffffc04003ff800a03ff000594ff87ffffff87f800ff001800ff001800ff001800fffffffffffffffffffffffff},
