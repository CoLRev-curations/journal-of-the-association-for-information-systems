   colrev_pdf_id                 = {cpid2:fffffffffdfffffff87ffc0ff8400007fdc0000ffffffffffffffffffe00007ffe00007ffffffffffff00fffffefa7fffffe7ffff800003ffc00003ffc00003ff800003ffc0ffffffc00003ff000000ff07ffffff0ff800ff001800ff001800ff001800ff001800ff001800ff001800ff00181ffffffffffffffffffffffffff},
