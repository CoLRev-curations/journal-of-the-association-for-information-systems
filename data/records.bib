   colrev_status                 = {md_prepared},
   colrev_masterdata_provenance  = {journal:DBLP.bib/000790;;
                                    title:DBLP.bib/000790;;
                                    year:DBLP.bib/000790;;
                                    volume:DBLP.bib/000790;;
                                    number:DBLP.bib/000790;;
