   colrev_data_provenance        = {doi:CROSSREF.bib/000797;;
                                    url:CROSSREF.bib/000797;;
                                    abstract:CROSSREF.bib/000797;;
                                    language:CROSSREF.bib/000797;;},
   doi                           = {10.17705/1JAIS.00825},
   author                        = {Kotlarsky, Julia and Oshri, Ilan and Sekulic, Nevena},
   journal                       = {Journal of the Association for Information Systems},
   title                         = {Digital Sustainability in Information Systems Research: Conceptual Foundations and Future Directions},
   year                          = {2023},
   volume                        = {24},
   number                        = {4},
   pages                         = {936--952},
   url                           = {https://aisel.aisnet.org/jais/vol24/iss4/9},
   abstract                      = {In this editorial, we develop the concept of digital sustainability for the IS community. By systematically reviewing the Green IT and Green IS literatures, we show that the IS field has lagged behind current discourse in practice and therefore lacks the conceptualization of the relationships between digital technologies and sustainability. Digital sustainability is defined in this editorial as the development and deployment of digital resources and artifacts toward improving the environment, society, and economic welfare. We hope that this editorial motivates IS researchers to engage in digital sustainability as an emerging research area.},
   language                      = {eng},
}

