   colrev_pdf_id                 = {cpid2:fffffffffdfffffff87ffc0ff8400007fdc0000fffffffffffe007fffe0000fffff01ffffe00007fff0000ffff0000fffffffffffffe7ffff800001ffc00003ffc00003ffc00003ffc00003ffc007ffff83d4bbffffffffff00180fff001800ff001800ff001800ff001800ff001800ff2effffffd5fffffffffffffffffffff},
