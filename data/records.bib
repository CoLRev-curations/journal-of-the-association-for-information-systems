   colrev_pdf_id                 = {cpid2:fffffffffdfffffff87ffc0ff8400007fdc0000ffffffffffffffffff001000fff0000ffffffffffffc003ffff8001fffffffffff800003ffc00003ff800003ffc00003ffc00003ffc00003ff800003ff800003ffc0000fffc0001fff000000ff07ffffff0ff800ff001800ff001800ff001800ffdb5ffffffffffffffffffff},
