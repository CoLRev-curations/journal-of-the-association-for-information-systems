   colrev_status                 = {md_processed},
   colrev_masterdata_provenance  = {CURATED:https://github.com/geritwagner/journal-of-the-association-for-information-systems;;},
   colrev_data_provenance        = {doi:https://api.crossref.org/works/10.17705/1JAIS.00695;;
                                    abstract:https://api.crossref.org/works/10.17705/1JAIS.00695;;
