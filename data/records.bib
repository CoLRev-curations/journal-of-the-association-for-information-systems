@article{DengGalliers2024,
   colrev_origin                 = {DBLP.bib/000882;},
   colrev_status                 = {md_prepared},
   colrev_masterdata_provenance  = {author:DBLP.bib/000882;;
                                    journal:DBLP.bib/000882;;
