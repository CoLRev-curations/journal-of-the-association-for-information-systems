@article{ViragBernroiderRemus2024,
   colrev_origin                 = {DBLP.bib/000869;
                                    pdfs.bib/000926;},
   colrev_status                 = {md_needs_manual_preparation},
   colrev_masterdata_provenance  = {author:pdfs.bib/000926;;
                                    journal:pdfs.bib/000926;;
                                    number:pdfs.bib/000926;;
                                    pages:pdfs.bib/000926;;
                                    volume:pdfs.bib/000926;;
                                    year:pdfs.bib/000926;;
                                    title:DBLP.bib/000869;language-unknown;},
   colrev_data_provenance        = {doi:pdfs.bib/000926;;
                                    file:pdfs.bib/000926;;
                                    colrev.dblp.dblp_key:DBLP.bib/000869;;
                                    url:DBLP.bib/000869;;
                                    abstract:http://dx.doi.org/10.17705/1JAIS.00880;;
                                    language:LanguageDetector;;},
   file                          = {data/pdfs/25_5/Agile Project Management Styles and Control Ambidexterity in Agil.pdf},
   doi                           = {10.17705/1JAIS.00880},
   colrev.dblp.dblp_key          = {https://dblp.org/rec/journals/jais/ViragBR24},
   author                        = {Virag, Peter and Bernroider, Edward W N and Remus, Ulrich},
   journal                       = {Journal of the Association for Information Systems},
   title                         = {Agile Project Management Styles and Control Ambidexterity in Agile Information Systems Development Projects: An Exploratory Case Study},
   year                          = {2024},
   volume                        = {25},
   number                        = {5},
   pages                         = {1274--1302},
   url                           = {https://aisel.aisnet.org/jais/vol25/iss5/5},
   abstract                      = {Agile information systems development (ISD) projects face the dilemma of control versus autonomy. Although autonomy benefits agile ISD, many projects in practice nevertheless feature project managers who exert control for more formal structure and guidance. To address this autonomy-control dilemma, prior research on traditional ISD highlights the need for control ambidexterity, which is the simultaneous execution of contrasting control activities. However, little is known about achieving control ambidexterity in agile ISD projects, and, in particular, how agile project managers dynamically adapt controls to changing contextual requirements. Our in-depth case study in the IT department of a multinational container shipping company that runs several Scrum ISD projects identifies four agile project management styles (Landscaper, Buddy, Detective, and Commander) and associated balanced practices for control ambidexterity. We also show how agile project managers blend or shift their styles in response to conflicts, revealing different levels of adherence to agile development principles. We contribute to the ISD control literature by reflecting on different forms of contextual and temporal control ambidexterity and theorizing how the interplay between control conflicts and underlying factors leads to varied ambidexterity forms. Furthermore, our insights suggest signaling theory should complement agency and stewardship theories to better understand agile ISD control.},
   language                      = {eng},
}

