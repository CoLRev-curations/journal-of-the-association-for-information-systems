@article{ChanLeeSkoumpopoulouEtAl2024a,
   colrev_origin                 = {CROSSREF.bib/000824;
                                    DBLP.bib/000826;
                                    pdfs.bib/000857;},
   colrev_status                 = {md_processed},
   colrev_masterdata_provenance  = {author:pdfs.bib/000857;;
                                    journal:pdfs.bib/000857;;
                                    number:pdfs.bib/000857;;
                                    pages:pdfs.bib/000857;;
                                    volume:pdfs.bib/000857;;
                                    year:pdfs.bib/000857;;
                                    title:CROSSREF.bib/000824;;},
   colrev_data_provenance        = {doi:pdfs.bib/000857;;
                                    file:pdfs.bib/000857;;
                                    url:CROSSREF.bib/000824;;
                                    abstract:CROSSREF.bib/000824;;
                                    language:CROSSREF.bib/000824;;
                                    colrev.dblp.dblp_key:DBLP.bib/000826;;},
   file                          = {data/pdfs/25_2/Judging the Wrongness of Firms in Social Media Firestorms_ The He.pdf},
   doi                           = {10.17705/1JAIS.00837},
   colrev.dblp.dblp_key          = {https://dblp.org/rec/journals/jais/ChanLSS24},
   author                        = {Chan, Tommy K H and Lee, Zach W Y and Skoumpopoulou, Dimitra and Situmeang, Frederik and Thatcher, Jason Bennett},
   journal                       = {Journal of the Association for Information Systems},
   title                         = {Judging the Wrongness of Firms in Social Media Firestorms: The Heuristic and Systematic Information Processing Perspective},
   year                          = {2024},
   volume                        = {25},
   number                        = {2},
   pages                         = {463--500},
   url                           = {https://aisel.aisnet.org/jais/vol25/iss2/1},
   abstract                      = {Social media firestorms pose a significant challenge for firms in the digital age. Tackling firestorms is difficult because the judgments and responses from social media users are influenced by not only the nature of the transgressions but also by the reactions and opinions of other social media users. Drawing on the heuristic-systematic information processing model, we propose a research model to explain the effects of social impact (the heuristic mode) and argument quality and moral intensity (the systematic mode) on perceptions of firm wrongness (the judgment outcome) as well as the effects of perceptions of firm wrongness on vindictive complaining and patronage reduction. We adopted a mixed methods approach in our investigation, including a survey, an experiment, and a focus group study. Our findings show that the heuristic and systematic modes of information processing exert both direct and interaction effects on individuals’ judgment. Specifically, the heuristic mode of information processing dominates overall and also biases the systematic mode. Our study advances the literature by offering an alternative explanation for the emergence of social media firestorms and identifying a novel context in which the heuristic mode dominates in dual information processing. It also sheds light on the formulation of response strategies to mitigate the adverse impacts resulting from social media firestorms. We conclude our paper with limitations and future research directions.},
   language                      = {eng},
}

