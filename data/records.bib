   colrev_data_provenance        = {url:DBLP.bib/000777;;
                                    language:http://dx.doi.org/10.17705/1JAIS.00786;;
                                    doi:https://aisel.aisnet.org/jais/vol24/iss2/2;;
                                    abstract:http://dx.doi.org/10.17705/1JAIS.00786;;
