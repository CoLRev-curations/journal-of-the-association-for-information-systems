                                    language:LanguageDetector;;},
   file                          = {data/pdfs/25_5/A Typology and Model of Privacy- and Security-Concerned Users_ At.pdf},
   doi                           = {10.17705/1JAIS.00893},
   colrev.dblp.dblp_key          = {https://dblp.org/rec/journals/jais/MuhammadDBAA24},
   author                        = {Muhammad, Syed Sardar and Dey, Bidit L and Bala, Hillol and Faridah, Sharifah and Alwi, Syed and Asaad, Yousra and Sardar,},
   journal                       = {Journal of the Association for Information Systems},
   title                         = {A Typology and Model of Privacy- and Security-Concerned Users' Attitudes toward Digital Footprints and the Consequent Influence on Their Social Media Adaptation},
   year                          = {2024},
   volume                        = {25},
   number                        = {5},
   pages                         = {1240--1273},
   url                           = {https://aisel.aisnet.org/jais/vol25/iss5/6},
   abstract                      = {How users with privacy and security concerns engage with social media in light of their perceptions of the risk associated with their digital footprint is a critical question for research and practice. Using a mixed methods approach, we examined the privacy- and security-related concerns of social media users and their subsequent adaptation behaviors in two studies. The first study, a qualitative inquiry, helped us develop a typology of four groups of social media users with respect to their privacy- and security-related attitudes characterized as careless, carefree, conscious, and cautious. We developed a conceptual model comprised of eight hypotheses based on a qualitative study that captured the relationship between privacy- and security-related attitudes and social media adaptation. The second study, a quantitative study, tested the model and revealed that users with careless and carefree attitudes are likely to explore social media to maximize benefits and exploit certain applications, while cautious users are likely to avoid using social media as much as possible. However, the findings are inconclusive for conscious users’ adaptation. This could be due to their systematic and informed decision-making, which is likely to have contextual variability. We contribute by offering a clear and coherent typology and a model of privacy- and security-concerned users’ attitudes and social media adaptation.},
   language                      = {eng},
}

@article{MukkamalaKauffmanHenriksen2023,
   colrev_origin                 = {CROSSREF.bib/000860;
